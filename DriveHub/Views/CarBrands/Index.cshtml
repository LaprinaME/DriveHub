@model IEnumerable<DriveHub.Models.CarBrand>

@{
    ViewData["Title"] = "Car Brands";
}

<h1>Car Brands</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Brand Name</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var brand in Model)
        {
            <tr>
                <td>@brand.BrandName</td>
                <td>@brand.Country</td>
            </tr>
        }
    </tbody>
</table>
